<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Template</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
      <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/css/site.css" />
    <link href="~/lib/mvc-grid/mvc-grid.css" rel="stylesheet" />
    <link href="~/lib/material-dashboard/css/material-dashboard.css" rel="stylesheet" />
</head>
<body>
<div class="wrapper">
    <partial name="_Sidebar" />
    <div class="main-panel">
        <partial name="_Topbar" />
        <div class="content">
            <div class="container-fluid">
                <div class="row">
                    @RenderBody()
                </div>
            </div>
        </div>
        <hr />
        <footer class="border-top footer text-muted">
            <div class="container">
                <p class="text-center">&copy; @DateTime.Now.Year - Template</p>
            </div>
        </footer>
    </div>
</div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/lib/mvc-grid/mvc-grid.js"></script>
      <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>

    <script src="~/lib/material-dashboard/js/material-dashboard.js"></script>
    <script src="~/lib/modal-helper.js"></script>
    <script src="~/js/template.js"></script>
    <partial name="_Modal" />
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
